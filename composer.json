{
    "name": "TNovalis/FiveM-ServerManager",
    "description": "FiveM Server Manager",
    "keywords": ["fivem", "game", "server", "manager"],
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom (Novalis)",
            "email": "contact@tnovalis.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "ext-sqlite3": "*",
        "illuminate/database": "5.7.*",
        "intonate/tinker-zero": "^1.0",
        "laravel-zero/framework": "5.7.*",
        "weidner/goutte": "^1.0",
        "zendframework/zend-text": "^2.7"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "ext-posix": "0"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["fsm"]
}
